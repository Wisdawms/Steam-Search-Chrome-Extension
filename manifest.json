{
    "manifest_version": 3,
    "name": "Steam Search Extension",
    "version": "1.0",
    "description": "Adds a context menu option to search selected text on the Steam store.",
    "permissions": ["contextMenus", "activeTab", "storage"],
    "icons": {
      "48": "icon.png"
    },
    "background":
    {
        "service_worker": "service_worker.js",
        "type": "module"
    }
  }
  